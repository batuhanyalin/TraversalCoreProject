<!DOCTYPE html>
<html lang="en">
<head>
    @await Component.InvokeAsync("_MemberHeadComponentPartial")
</head>

<body class="g-sidenav-show  bg-gray-100">
    @await Component.InvokeAsync("_MemberSidebarComponentPartial")
    <main class="main-content position-relative max-height-vh-100 h-100 mt-1 border-radius-lg ">
        <!-- Navbar -->
        @await Component.InvokeAsync("_MemberNavbarComponentPartial")
        <!-- End Navbar -->
        <div class="container-fluid py-4">
            @RenderBody()
            <!-- Footer -->
            @await Component.InvokeAsync("_MemberFooterComponentPartial")
        </div>
    </main>
    @await Component.InvokeAsync("_MemberSettingScriptComponentPartial")
    <!--   Core JS Files   -->
    @await Component.InvokeAsync("_MemberScriptsComponentPartial")
</body>

</html>