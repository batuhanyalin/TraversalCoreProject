@model List<DestinationMatchGuideViewModel>
@for (int i = 0; i < Model.Count; i++)
{
    <div class="col">
        <div class="form-check">
            <input type="hidden" asp-for="@Model[i].DestinationId" />
            <input type="hidden" asp-for="@Model[i].GuideId" />
            <a href="/Admin/Guide/UpdateGuide/@Model[i].GuideId"><img height="70px" width="60px" src="@Model[i].GuideImageUrl" /></a>
           <input type="checkbox" asp-for="@Model[i].GuideExist" class="form-check-input" />
            <label class="form-check-label" for="@Model[i].GuideExist">@Model[i].GuideName</label>
        </div>
    </div>
}