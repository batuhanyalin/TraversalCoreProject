@model List<DestinationMatchGuide>
<ul class="list-unstyled users-list m-0 avatar-group d-flex align-items-center">
    @foreach (var item in Model)
    {
        <li data-bs-toggle="tooltip"
            data-popup="tooltip-custom"
            data-bs-placement="top"
            class="avatar avatar-xs pull-up"
            title="@item.Destination.City.Country.Continent.ContinentName | @item.Destination.City.Country.CountryName - @item.Destination.City.CityName">
            <a href="/Destination/DestinationDetail/@item.Destination.DestinationId">
                <img src="@item.Destination.ImageUrl" alt="Avatar" class="rounded-circle" />
            </a>
        </li>
    }
</ul>