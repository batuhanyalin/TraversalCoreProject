@model DashboardStatisticCardModel
<div class="card">
    <div class="card-body">
        <div class="card-title d-flex align-items-start justify-content-between">
            <div class="avatar flex-shrink-0">
               <i style="font-size:xx-large" class=" fas fa-map-marked-alt"></i>
            </div>
        </div>
        <span class="fw-semibold d-block mb-1">Tur Sayısı</span>
        <h3 class="card-title mb-2">@Model.StatisticCardCount</h3>
        <small class="text-success fw-semibold"><i class="bx bx-up-arrow-alt"></i> +72.80%</small>
    </div>
</div>